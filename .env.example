# Orbia Backend Environment Variables Example
# Copy this file to .env and update with your actual values

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================

# Disable authentication for development (set to "true" to disable auth)
DISABLE_AUTH=false

# Logging configuration
CONCISE_LOGGING=false

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# PostgreSQL database connection string
# Format: postgresql://username:password@host:port/database_name
DATABASE_URL=postgresql://username:password@localhost:5432/orbia_db
# For Mem0
POSTGRES_USERNAME=
POSTGRES_PASSWORD=
POSTGRES_HOST=
POSTGRES_PORT=

# =============================================================================
# REDIS CONFIGURATION
# =============================================================================

# Redis connection settings
REDIS_HOST=localhost
REDIS_PORT=6379

# =============================================================================
# JWT AUTHENTICATION
# =============================================================================

# Secret key for JWT token signing and verification
JWT_SECRET_KEY=your-super-secret-jwt-key-here-change-this-in-production

# =============================================================================
# PROMETHEUS MONITORING
# =============================================================================

# Basic auth credentials for Prometheus metrics endpoint
PROMETHEUS_USERNAME=admin
PROMETHEUS_PASSWORD=secure-prometheus-password

# =============================================================================
# AI MODEL API KEYS
# =============================================================================

# OpenAI API configuration
OPENAI_API_KEY=sk-your-openai-api-key-here

# Anthropic Claude API
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# DeepSeek API
DEEPSEEK_API_KEY=your-deepseek-api-key-here

# Groq API
GROQ_API_KEY=your-groq-api-key-here

# Google Gemini API
GEMINI_API_KEY=your-gemini-api-key-here

# Fireworks AI API
FIREWORKS_API_KEY=your-fireworks-api-key-here

# Google Vertex AI Service Account (JSON string or file path)
VERTEX_AI_SERVICE_ACCOUNT={"type": "service_account", "project_id": "your-project"}


LANGFUSE_ENABLED=true
LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key-here
LANGFUSE_SECRET_KEY=sk-lf-your-secret-key-here
LANGFUSE_BASE_URL=https://cloud.langfuse.com

LANGFUSE_PROMPTS_FETCH_ENABLED=true
LANGFUSE_PROMPT_CACHE_TTL_IN_SEC=60
DEBUG=false
ENVIRONMENT=development
